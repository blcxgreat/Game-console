module movement (Clock, reset, count, LEFT, RIGHT, UP, DOWN, L, R, U, D, tracking, gameover);
	input logic Clock, reset;
	input logic LEFT, RIGHT, UP, DOWN;
	input logic gameover;
	output logic L, R, U, D;
	output logic tracking;
	input reg [2:0] count;
	logic [9:0] nowMoveCount; //1011111011
	logic track_num;
	
	enum { None, Left, Right, Up, Down} ps, ns;
	
	always_ff @(posedge Clock) begin          
      if (reset) begin
			ps <= None;
			nowMoveCount <= 10'b0000000000;
			tracking <= 0;
			end
		else begin
			ps <= ns;
			tracking <= track_num;
			if (nowMoveCount == 10'b1011111011)
				nowMoveCount <= 10'b0000000000;
			else if (count == 3'b111)
				nowMoveCount <= nowMoveCount + 10'b0000000001;
			end
   end
	
	always_comb begin
		case (ps)
			None:	if (LEFT & ~RIGHT & ~UP & ~DOWN) 			ns = Left;
					else if (RIGHT & ~LEFT & ~UP & ~DOWN)	ns = Right;
					else if (UP & ~LEFT & ~RIGHT & ~DOWN)		ns = Up;
					else if (DOWN & ~LEFT & ~RIGHT & ~UP)	ns = Down;
					else			 															ns = None;
					
			Left:	if (UP & ~LEFT & ~RIGHT & ~DOWN)			ns = Up;
					else if (DOWN & ~LEFT & ~RIGHT & ~UP)	ns = Down;
					else			 															ns = Left;
					
			Right:if (UP & ~LEFT & ~RIGHT & ~DOWN)			ns = Up;
					else if (DOWN & ~LEFT & ~RIGHT & ~UP)	ns = Down;
					else			 															ns = Right;
					
			Up:	if (LEFT & ~RIGHT & ~UP & ~DOWN) 			ns = Left;
					else if (RIGHT & ~LEFT & ~UP & ~DOWN)	ns = Right;
					else			 															ns = Up;
					
			Down:	if (LEFT & ~RIGHT & ~UP & ~DOWN) 			ns = Left;
					else if (RIGHT & ~LEFT & ~UP & ~DOWN)	ns = Right;
					else			 															ns = Down;
		endcase
	end
	
	assign L = (((ps == Left) && (nowMoveCount == 10'b1011111011)) & ~gameover);
	assign R = (((ps == Right) && (nowMoveCount == 10'b1011111011)) & ~gameover);
	assign U = (((ps == Up) && (nowMoveCount == 10'b1011111011)) & ~gameover);
	assign D = (((ps == Down) && (nowMoveCount == 10'b1011111011)) & ~gameover);
	assign track_num = (nowMoveCount == 10'b1011111011);
	
endmodule







